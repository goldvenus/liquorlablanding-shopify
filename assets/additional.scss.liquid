.partial-button-parent {
  cursor: pointer;
  .partial-button {
    width: 200px;
    height: 45px;
    border: 4px solid red;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .partial-button:before {
    content: '';
    position: absolute;
    display: block;
    width: 25px;
    bottom: -6px;
    left: -6px;
    border: 15px solid white;
  }
  .partial-button:after {
    content: '';
    position: absolute;
    display: block;
    width: 25px;
    top: -6px;
    right: -6px;
    border: 15px solid white;
  }
  .partial-button span {
    font-size: 30px;
    text-align: center;
    z-index: 100;
    position: absolute;
  }
}
